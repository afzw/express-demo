    "@types/compression": "^1.7.2",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.14.171",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
