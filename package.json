{
  "name": "express-demo",
  "version": "1.0.0",
  "description": "web后台服务",
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  },
  "scripts": {
    "prepare": "husky install",
    "init": "chmod +x ./init.sh | ./init.sh",
